{
  "node_version": "20",
  "graphs": {
    "agent": "./src/agent/graph.ts:graph"
  },
  "env": ".env",
  "dependencies": ["."],
  "http": {
    "cors": {
      "allow_origins": [
        "http://localhost:3000",
        "https://youquantified.com",
        "https://api.smith.langchain.com"
      ],
      "allow_methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
      ],
      "allow_headers": [
        "Content-Type",
        "Authorization"
      ],
      "allow_credentials": true,
      "max_age": 600
    }
  }
}